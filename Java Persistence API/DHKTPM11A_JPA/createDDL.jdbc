CREATE TABLE ADDRESS (STREETADDRESS VARCHAR(255) NOT NULL, CITY VARCHAR(255) NULL, POSTALCODE VARCHAR(255) NULL, STATE VARCHAR(255) NULL, STUDENT_studentId NUMERIC(19) NULL, PRIMARY KEY (STREETADDRESS))
CREATE TABLE STUDENT (studentId NUMERIC(19) NOT NULL, DATEOFBIRTH DATETIME NULL, studentName VARCHAR(150) NOT NULL, CLS_CLASSID NUMERIC(19) NULL, ADDRESS_STREETADDRESS VARCHAR(255) NULL, PRIMARY KEY (studentId))
CREATE TABLE StudentClass (CLASSID NUMERIC(19) IDENTITY NOT NULL, className VARCHAR(200) NOT NULL, PRIMARY KEY (CLASSID))
ALTER TABLE ADDRESS ADD CONSTRAINT DDRESSSTUDENTstudentId FOREIGN KEY (STUDENT_studentId) REFERENCES STUDENT (studentId)
ALTER TABLE STUDENT ADD CONSTRAINT STDNTDDRSSSTRETADDRESS FOREIGN KEY (ADDRESS_STREETADDRESS) REFERENCES ADDRESS (STREETADDRESS)
ALTER TABLE STUDENT ADD CONSTRAINT FK_STUDENT_CLS_CLASSID FOREIGN KEY (CLS_CLASSID) REFERENCES StudentClass (CLASSID)
